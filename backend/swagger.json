{
  "openapi": "3.0.0",
  "info": {
    "title": "MRE",
    "version": "1.0.0",
    "description": "Mirae Real Estate"
  },
  "tags": [
    { "name": "apartment" },
    { "name": "officetel/oneroom/tworoom" },
    { "name": "house" },
    { "name": "commercial/store" },
    { "name": "factory/forest/land" }
  ],
  "paths": {
    "/api/apartment": {
      "get": {
        "tags": ["apartment"],
        "summary": "Get a list of entire apartments",
        "description": "Get a list of apartments for all types of sale methods",
        "requestBody": {
          "description": "A list of apartements",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#components/schemas/Property"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Property": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int",
            "example": 10
          },
          "urgent_sale": {
            "type": "integer",
            "format": "tinyint1",
            "example": 1
          },
          "property_name": {
            "type": "string",
            "format": "var50",
            "example": "에비씨 아파트"
          },
          "property_type": {
            "type": "string",
            "format": "var20",
            "example": "apartment"
          },
          "sale_method": {
            "type": "string",
            "format": "var20",
            "example": "sale"
          },
          "sale_price": {
            "type": "string",
            "format": "var20",
            "example": "10억"
          },
          "deposit": {
            "type": "string",
            "format": "var20",
            "example": "500만"
          },
          "premium_cost": {
            "type": "string",
            "format": "var20",
            "example": "3000만"
          },
          "management_cost": {
            "type": "string",
            "format": "var20",
            "example": "10만"
          },
          "price_nego": {
            "type": "integer",
            "format": "tinyint1",
            "example": "sale"
          },
          "gross_leasable_area": {
            "type": "integer",
            "format": "int",
            "example": "100"
          },
          "exclusive_area": {
            "type": "integer",
            "format": "int",
            "example": "80"
          },
          "building_area": {
            "type": "integer",
            "format": "int",
            "example": "300"
          },
          "land_area": {
            "type": "integer",
            "format": "int",
            "example": "1000"
          },
          "postal_code": {
            "type": "string",
            "format": "var30",
            "example": "아차산로 31길 50"
          },
          "detail_address": {
            "type": "string",
            "format": "var50",
            "example": "화양 에비씨아파트 201동 201호"
          },
          "floor_plan": {
            "type": "string",
            "format": "var255",
            "example": "https://www.google.com/search?q=445%ED%8F%89&rlz=1C5CHFA_enKR1023KR1031&oq=445%ED%8F%89&gs_lcrp=EgZjaHJvbWUyBggAEEUYOTIHCAEQABiiBDIHCAIQABiiBDIHCAMQABiiBNIBCDI0OTFqMGo3qAIAsAIA&sourceid=chrome&ie=UTF-8"
          },
          "property_picture": {
            "type": "string",
            "format": "var255",
            "example": "https://www.google.com/search?q=445%ED%8F%89&rlz=1C5CHFA_enKR1023KR1031&oq=445%ED%8F%89&gs_lcrp=EgZjaHJvbWUyBggAEEUYOTIHCAEQABiiBDIHCAIQABiiBDIHCAMQABiiBNIBCDI0OTFqMGo3qAIAsAIA&sourceid=chrome&ie=UTF-8"
          },
          "year_built": {
            "type": "integer",
            "format": "int",
            "example": "2010"
          },
          "availability": {
            "type": "string",
            "format": "var10",
            "example": "즉시 가능"
          },
          "parking": {
            "type": "string",
            "format": "var10",
            "example": "1대, 주차 가능"
          },
          "direction": {
            "type": "string",
            "format": "var10",
            "example": "아차산로 31길 50"
          },
          "floor": {
